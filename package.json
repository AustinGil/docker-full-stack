{
  "name": "node-sandbox",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "migrate:make": "knex migrate:make --",
    "migrate:latest": "knex migrate:latest",
    "seed:make": "knex seed:make --",
    "seed:run": "knex seed:run",
    "docker:start": "docker-compose up -d",
    "docker:restart": "docker-compose restart",
    "docker:stop": "docker-compose down",
    "docker:rebuild": "docker-compose up --build -d",
    "docker:ssh": "docker exec -i -t node-sandbox_node_1 /bin/bash",
    "dev": "nodemon index.js",
    "local:dev": "NODE_ENV=\"local\" nodemon index.js",
    "local:migrate": "NODE_ENV=\"local\" knex migrate:latest",
    "local:seed": "NODE_ENV=\"local\" knex seed:run"
  },
  "dependencies": {
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "knex": "^0.15.2",
    "mysql": "^2.16.0",
    "sqlite3": "^4.0.2"
  },
  "devDependencies": {
    "nodemon": "^1.17.5"
  }
}
